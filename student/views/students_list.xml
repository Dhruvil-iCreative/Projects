<odoo>
    <template id="students_list" name="Students List">
        <t t-call="website.layout">
            <title>Students List</title>
            <center>
                <h1>Students List</h1>
            </center>
            <br/>
                <br/>
                <div class="page">
                    <div class="col-xs-8">
                        <table class="table table-sm o_main_table">
                            <thead style="display: table-row-group">
                                <tr>
                                    <th name="th_name" class="text-left">
                                        Name
                                    </th>
                                    <th name="th_organization_id" class="text-left">
                                        Organization
                                    </th>
                                    <th name="th_amount" class="text-left">
                                        Amount
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach="docs" t-as="doc">
                                    <tr>
                                        <td>
                                            <span t-field="doc.name"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.organization_id"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.amount"/>
                                        </td>
                                    </tr>
                                </t>
                            </tbody>
                        </table>
                    </div>
                </div>
        </t>
<!--                 <h1>Hello</h1>-->
    </template>
</odoo>